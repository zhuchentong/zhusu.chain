{
  "name": "app",
  "version": "0.0.1",
  "author": "Ionic Framework",
  "homepage": "http://ionicframework.com/",
  "scripts": {
    "dev": "ng serve --open",
    "dev:android": "ionic cordova run android -lc --debug",
    "dev:ios": "ionic cordova run ios -lc",
    "android": "ionic cordova run android",
    "publish:android": "ionic cordova build android",
    "start": "ng serve",
    "build": "ng build",
    "lint": "ng lint",
    "test": "ng test",
    "e2e": "ng e2e",
    "commit": "commit"
  },
  "husky": {
    "hooks": {
      "pre-commit": "ng lint",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": ".cz-config.js"
    }
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~7.2.0",
    "@angular/common": "~7.2.1",
    "@angular/core": "~7.2.1",
    "@angular/forms": "~7.2.1",
    "@angular/http": "~7.2.1",
    "@angular/platform-browser": "~7.2.1",
    "@angular/platform-browser-dynamic": "~7.2.1",
    "@angular/router": "~7.2.1",
    "@ionic-native/core": "5.0.0-beta.23",
    "@ionic-native/geolocation": "5.0.0-beta.23",
    "@ionic-native/social-sharing": "^5.2.0",
    "@ionic-native/splash-screen": "5.0.0-beta.23",
    "@ionic-native/status-bar": "5.0.0-beta.23",
    "@ionic/angular": "4.0.0-rc.1",
    "@ionic/storage": "~2.2.0",
    "@ngx-toolkit/logger": "~7.0.1",
    "@ngxs/storage-plugin": "~3.3.4",
    "@ngxs/store": "~3.3.4",
    "class-transformer": "~0.2.0",
    "coordtransform": "~2.1.2",
    "cordova-android": "7.1.4",
    "cordova-plugin-device": "^2.0.2",
    "cordova-plugin-ionic-keyboard": "^2.1.3",
    "cordova-plugin-ionic-webview": "^3.0.0",
    "cordova-plugin-splashscreen": "^5.0.2",
    "cordova-plugin-statusbar": "^2.2.1",
    "cordova-plugin-whitelist": "^1.3.3",
    "cordova-plugin-x-socialsharing": "5.4.4",
    "core-js": "~2.6.2",
    "dayjs": "~1.8.0",
    "es6-promise-plugin": "4.2.2",
    "ethers": "~4.0.21",
    "lodash.clone": "~4.5.0",
    "lodash.clonedeep": "~4.5.0",
    "ngx-amap": "~2.2.0",
    "ngx-clipboard": "~11.1.9",
    "ngx-pipes": "~2.3.6",
    "qs": "^6.6.0",
    "reflect-metadata": "~0.1.13",
    "rxjs": "~6.3.3",
    "spritejs": "^2.27.3",
    "uuid": "^3.3.2",
    "zone.js": "~0.8.27"
  },
  "devDependencies": {
    "@angular-devkit/architect": "~0.12.1",
    "@angular-devkit/build-angular": "~0.12.1",
    "@angular-devkit/core": "~7.2.1",
    "@angular-devkit/schematics": "~7.2.1",
    "@angular/cli": "~7.2.1",
    "@angular/compiler": "~7.2.1",
    "@angular/compiler-cli": "~7.2.1",
    "@angular/language-service": "~7.2.1",
    "@commitlint/cli": "~7.3.1",
    "@ionic/angular-toolkit": "~1.2.0",
    "@ngxs/devtools-plugin": "~3.3.4",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~10.12.0",
    "codelyzer": "~4.5.0",
    "commitizen": "~3.0.5",
    "commitlint-config-cz": "~0.11.1",
    "cz-conventional-changelog": "~2.1.0",
    "cz-customizable": "~5.3.0",
    "husky": "~1.3.1",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.1.4",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "~0.2.2",
    "nodemon": "~1.18.9",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.12.0",
    "tslint-config-prettier": "~1.17.0",
    "tslint-config-standard": "~8.0.1",
    "typescript": "~3.2.2"
  },
  "description": "An Ionic project",
  "cordova": {
    "plugins": {
      "cordova-plugin-whitelist": {},
      "cordova-plugin-device": {},
      "cordova-plugin-splashscreen": {},
      "cordova-plugin-ionic-webview": {
        "ANDROID_SUPPORT_ANNOTATIONS_VERSION": "27.+"
      },
      "cordova-plugin-ionic-keyboard": {},
      "cordova-plugin-x-socialsharing": {
        "ANDROID_SUPPORT_V4_VERSION": "24.1.1+"
      },
      "cordova-plugin-statusbar": {}
    },
    "platforms": [
      "android"
    ]
  }
}
